package JavaBasico.estructurasdecontrol.repetitivas;

public class Ejercicio3 {
    public static void main(String[] args) {

        String[] texto = {"Sol solecito\n" +
                "Caliéntame un poquito,\n" +
                "Por hoy por mañana\n" +
                "Por toda la semana\n" +
                "Luna lunera cascabelera\n" +
                "Cinco pollitos y una ternera\n" +
                "Caracol, caracol\n" +
                "A la una sale el sol. "};
        for (int i = 0; i < texto.length; i++) {
            System.out.println(texto[i]);
        }

    }

}
